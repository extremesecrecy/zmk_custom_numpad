#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
&kp LOCKING_NUM  &kp SLASH  &kp STAR  &kp MINUS
         &kp N7     &kp N8    &kp N9   &kp PLUS
         &kp N4     &kp N5    &kp N6  &kp ENTER
         &kp N1     &kp N2    &kp N3     &kp N0
            >;
        };
    };
};